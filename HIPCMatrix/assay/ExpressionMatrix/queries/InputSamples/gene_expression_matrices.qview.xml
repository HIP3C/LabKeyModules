<cv:customView xmlns:cv="http://labkey.org/data/xml/queryCustomView" containerFilter="CurrentAndSubfolders">
    <cv:columns>
        <cv:column name="Run/Name">
         <cv:properties>
            <cv:property name="columnTitle" value="Name"/>
          </cv:properties>
        </cv:column>
        <cv:column name="Biosample/biosample_accession"/>
        <cv:column name="Biosample/ParticipantId"/>
        <cv:column name="Biosample/arm_name">
         <cv:properties>
            <cv:property name="columnTitle" value="Cohort"/>
          </cv:properties>
        </cv:column>
        <cv:column name="Biosample/study_time_collected"/>
        <cv:column name="Biosample/study_time_collected_unit"/>
        <cv:column name="Run/featureSet"/>
        <cv:column name="Run/DataOutputs/viewFileLink"/>
    </cv:columns>
    <cv:filters>
        <cv:filter column="Biosample/biosample_accession" operator="startswith" value="BS"/>
    </cv:filters>
</cv:customView>
