<cv:customView xmlns:cv="http://labkey.org/data/xml/queryCustomView" label="Differential gene expression analysis results" category="Assays" showInDataViews="true">
    <cv:columns>
        <cv:column name="analysis_accession"/>
        <cv:column name="analysis_accession/arm_name"/>
        <cv:column name="analysis_accession/coefficient"/>
        <cv:column name="feature_id"/>
        <cv:column name="gene_symbol">
            <!--<facetingBehavior>"ALWAYS_ON"</facetingBehavior>-->
            <!--<cv:columnType name="FacetingBehaviorType" value="ALWAYS_ON"/>-->
        </cv:column>
        <cv:column name="adj_p_val"/>
        <cv:column name="ave_expr"/>
        <cv:column name="log_fc"/>
        <cv:column name="p_value"/>
        <cv:column name="statistic"/>
    </cv:columns>
    <cv:filters>
        <cv:filter column="gene_symbol" operator="neq" value="NA"/>
    </cv:filters>
</cv:customView>

