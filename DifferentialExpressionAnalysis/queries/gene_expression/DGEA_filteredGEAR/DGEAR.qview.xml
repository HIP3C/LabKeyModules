<cv:customView xmlns:cv='http://labkey.org/data/xml/queryCustomView'  containerFilter='CurrentAndSubfolders' label='Differential gene expression analysis results' category='Assays' showInDataViews='true'>
    <cv:columns>
        <cv:column name='analysis_accession'/>
        <cv:column name='cohort'/>
        <cv:column name='coefficient'/>
        <cv:column name='feature_id'/>
        <cv:column name='gene_symbol'/>
        <cv:column name='adj_p_val'/>
        <cv:column name='ave_expr'/>
        <cv:column name='log_fc'/>
        <cv:column name='p_value'/>
        <cv:column name='statistic'/>
    </cv:columns>
    <cv:filters>
        <cv:filter column='gene_symbol' operator='neq' value='NA'/>
    </cv:filters>
</cv:customView>

